{
  "version": 2,
  "builds": [
    {
      "src": "Frontend/package.json",
      "use": "@vercel/vite",
      "config": {
        "outputDirectory": "Frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/Frontend/dist/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.[a-z0-9]+)$",
      "dest": "/Frontend/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/dist/index.html",
      "status": 200
    }
  ]
}
